@typeparam TItem

@*Normally put these in the Shared folder.*@

<ul>
    @foreach (var item in Items ?? Array.Empty<TItem>())
    {
        <li>@ChildContent(item) </li>
    }
</ul>

@code {
    [Parameter]
    public IEnumerable<TItem> Items { get; set; }

    [Parameter]
    public RenderFragment<TItem> ChildContent { get; set; }
}
